/*
作者:eva66
题目:p1201 最小数和最大数
*/

#include <iostream>
using namespace std;
int main()
{
	int i;
	int n;
	cin>>n;
	int a[100];
    
     for(i=0;i<n;i++)
     	{
     		cin>>a[i];
     	}
    int max=a[0];
    int min=a[0];
     for(i=0;i<n;i++)	
     {
        if(a[i]>max) max=a[i];
     		if(a[i]<min) min=a[i]; 
     }

     cout<<min<<" "<<max<<endl;
    return 0;
}


